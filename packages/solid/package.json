{
  "name": "@medplum/solid",
  "version": "5.0.10",
  "description": "Medplum SolidJS Component Library",
  "keywords": [
    "medplum",
    "fhir",
    "healthcare",
    "interoperability",
    "solidjs",
    "solid-component",
    "emr"
  ],
  "homepage": "https://www.medplum.com/",
  "bugs": {
    "url": "https://github.com/medplum/medplum/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medplum/medplum.git",
    "directory": "packages/solid"
  },
  "license": "Apache-2.0",
  "author": "Medplum <hello@medplum.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "solid": "./src/index.ts",
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./styles": "./dist/styles/index.css"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist/cjs",
    "dist/esm",
    "dist/styles"
  ],
  "scripts": {
    "api-extractor": "api-extractor run --local && cp dist/types.d.ts dist/cjs/index.d.ts && cp dist/types.d.ts dist/esm/index.d.ts",
    "build": "npm run clean && tsc && node esbuild.mjs && npm run build:css && npm run api-extractor",
    "build:css": "tailwindcss -i ./src/styles/index.css -o ./dist/styles/index.css --minify",
    "clean": "rimraf dist",
    "dev": "vite",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@kobalte/core": "^0.13.11",
    "@ark-ui/solid": "^4.0.0",
    "lucide-solid": "^0.469.0",
    "signature_pad": "^4.1.2"
  },
  "devDependencies": {
    "@medplum/core": "5.0.10",
    "@medplum/definitions": "5.0.10",
    "@medplum/fhirtypes": "5.0.10",
    "@medplum/mock": "5.0.10",
    "@medplum/solid-hooks": "5.0.10",
    "@solidjs/testing-library": "^0.8.9",
    "@solidjs/router": "^0.15.4",
    "@tanstack/solid-query": "^5.90.19",
    "@testing-library/jest-dom": "6.9.1",
    "@types/node": "22.19.3",
    "daisyui": "^5.0.0",
    "dotenv": "17.2.3",
    "esbuild-plugin-solid": "^0.6.0",
    "happy-dom": "^15.0.0",
    "solid-js": "^1.9.10",
    "storybook-solidjs-vite": "^1.0.0-beta.6",
    "storybook": "^8.6.15",
    "@storybook/addon-docs": "^8.6.15",
    "@storybook/addon-a11y": "^8.6.15",
    "@storybook/types": "^8.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-solid": "^2.11.10",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@medplum/core": "5.0.10",
    "@medplum/solid-hooks": "5.0.10",
    "@tanstack/solid-query": "^5.90.19",
    "solid-js": "^1.0.0"
  },
  "engines": {
    "node": "^22.18.0 || >=24.2.0"
  }
}
